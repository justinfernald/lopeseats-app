{
  "appId": "com.lopeseat.app",
  "appName": "LopesEat",
  "bundledWebRuntime": false,
  "windowsAndroidStudioPath": "C:\\Program Files\\Android\\Android Studio\\bin\\studio64.exe",
  "npmClient": "npm",
  "webDir": "build",
  "server": {
    "allowNavigation": [
      "lopeseat.com",
      "*.lopeseat.com"
    ]
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 0
    },
    "PushNotifications": {
      "presentationOptions": [
        "badge",
        "sound",
        "alert"
      ]
    },
    "LocalNotifications": {
      "smallIcon": "@drawable/ic_notification_lope",
      "iconColor": "#eb1c34"
    }
  },
  "backgroundColor": "#ffffffff",
  "android": {
    "webContentsDebuggingEnabled": true
  },
  "cordova": {}
}